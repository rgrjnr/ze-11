{
  "name": "ze-11",
  "version": "1.0.0",
  "description": "11ty project with Tailwind CSS and TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:scss && npm run build:ts && npm run build:11ty",
    "build:scss": "sass src/styles/input.scss:_site/assets/css/style.css --style=compressed --no-source-map",
    "build:scss:watch": "sass src/styles/input.scss:_site/assets/css/style.css --watch",
    "build:ts": "tsc",
    "build:11ty": "eleventy",
    "watch": "npm run build:scss:watch & npm run watch:ts & npm run watch:11ty",
    "watch:ts": "tsc --watch",
    "watch:11ty": "eleventy --watch --serve",
    "dev": "npm run watch",
    "clean": "rm -rf _site dist",
    "serve": "eleventy --serve"
  },
  "keywords": [
    "11ty",
    "eleventy",
    "tailwind",
    "typescript",
    "scss"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^3.1.1",
    "gsap": "^3.13.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2"
  }
}
